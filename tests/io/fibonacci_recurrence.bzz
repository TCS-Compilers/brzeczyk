// INTEGRATION TESTS IGNORE
czynność policz_liczbę_fibonacciego(którą: Liczba) -> Liczba {
    jeśli (którą <= 2) {
        zwróć 1
    }
    zwróć policz_liczbę_fibonacciego(którą - 2) + policz_liczbę_fibonacciego(którą - 1)
}

czynność główna() {
    wart wejście: Liczba = wczytaj()
    napisz(policz_liczbę_fibonacciego(wejście))
}

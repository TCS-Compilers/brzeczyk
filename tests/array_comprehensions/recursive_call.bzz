// USE VALGRIND

czynność dodaj_na_początku_ciągu(v: Liczba, c: [Liczba]) {
    przekaźnik tmp(): Liczba {
        przekaż v
        dla (x: Liczba wewnątrz c)
            przekaż x
    }
    zwróć ciąg [Liczba] (x otrzymując x: Liczba od tmp())
}

przekaźnik coraz_dłuższych_ciągów_do(n: Liczba): [Liczba] {
    jeśli (n == 1) {
        zwróć ciąg[[Liczba]] {ciąg [Liczba] {1}}
    }
    zm c: [[Liczba]] = ciąg [[Liczba]] (dodaj_na_początku_ciągu(1, x) otrzymując x: Liczba coraz_dłuższych_ciągów_do(n-1))

    otrzymując x: [Liczba] od coraz_dłuższych_ciągów_do(1234) {
        prekaż x // [1]
        przerwij
    }
    
    dla x: Liczba wewnątrz c {
        przekaż x
    }
}

czynność główna() {
    zm n: Liczba = wczytaj()
    otrzymując x: [Liczba] od coraz_dłuższych_ciągów_do(n) {
        zm v: Liczba = 0
        dla y: Liczba wewnątrz x {
            v = v*10+y
        }
        napisz(v)
    }
}

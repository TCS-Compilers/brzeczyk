przekaźnik g1() -> Czy {
    otrzymując x: Czy od g2 { 
        jeśli(x == fałsz)
            przekaż fałsz
    }
}

czynność główna() {
    zm lim: Liczba = wczytaj()
    zm cnt: Liczba = 0
    otrzymując x: Czy od g1(n) { 
        jeśli (x == fałsz)
            cnt = cnt+1
        jeśli(cnt == lim)
            zakończ
    }
}